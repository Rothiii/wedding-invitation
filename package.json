{
  "name": "wedding",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run dev:client\" \"bun run dev:server\"",
    "dev:client": "vite",
    "dev:server": "bun --watch src/server/index.ts",
    "dev:server:old": "bun src/server/server.js",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "bun src/server/index.ts",
    "server:old": "bun src/server/server.js",
    "generate-links": "bun generate-links-example.js",
    "deploy": "bun run build && wrangler deploy",
    "cf:dev": "wrangler dev",
    "cf:deploy": "wrangler deploy",
    "cf:tail": "wrangler tail",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@hono/node-server": "^1.8.0",
    "caniuse-lite": "^1.0.30001757",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "framer-motion": "^11.15.0",
    "hono": "^4.0.0",
    "lucide-react": "^0.469.0",
    "pg": "^8.16.3",
    "postgres": "^3.4.8",
    "react": "^18.3.1",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.1.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251128.0",
    "@eslint/js": "^9.17.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "vite": "^6.0.5",
    "wrangler": "^4.51.0"
  }
